	<!-- Bruce Zheng's Custom Code -->
	<script>
	function resize_profile_pics() {
		var blurb_height = $('.profile-blurb').height();
		$('#profile-img-lg').height(Math.min($('#profile-pic-lg').width(), blurb_height * .95));
		$('#profile-pic-lg').height(blurb_height);
		
		var xs_height = Math.min($('#profile-pic-xs').width(),1.33*blurb_height);
		$('#profile-pic-xs').height(xs_height);
		$('#profile-img-xs').height(xs_height * .85);
	}
	
	$( document ).ready(function() {
		resize_profile_pics()
		var msg, hr = new Date().getHours();
		if(hr < 5 || hr > 22) msg = "Greetings, night owl";
		else if (hr < 12 ) msg = "Good morning";
		else if (hr < 18) msg = "Good afternoon";
		else msg = "Good evening";
		$('#welcome_msg').text(msg);
	});
	
	$( window ).resize(function() {
		resize_profile_pics()
	});
	
	$( '.carousel' ).carousel({
		interval: false
	})
	</script>
